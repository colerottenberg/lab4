library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

-- Datapath for fibonacci sequence

entity datapath is
    port(
        clk : in std_logic;
        reset : in std_logic;
        start : in std_logic;
        done : out std_logic;
        fib : out unsigned(31 downto 0)
    );
end entity;